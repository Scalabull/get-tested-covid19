{
    "name": "get-tested-covid19",
    "version": "1.1.0",
    "description": "Website for the public to find COVID19 testing centers.",
    "main": "index.js",
    "repository": {
        "type": "git"
    },
    "keywords": [
        "react",
        "reactjs",
        "argon",
        "argon-react",
        "design",
        "design-react",
        "argon-design",
        "argon-design-react",
        "kit",
        "react-kit",
        "argon-design-system",
        "argon-design-system-react",
        "design-system-react"
    ],
    "author": "Zach Boldyga",
    "license": "MIT",
    "homepage": "https://get-tested-covid19.org",
    "scripts": {
        "dev:clean": "rm -rf node_modules && rm yarn.lock",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "test:dev": "react-scripts test --watchAll",
        "test:coverage": "jest --coverage",
        "eject": "react-scripts eject",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
        "compile-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css",
        "minify-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.min.css --output-style compressed",
        "map-sass": "node-sass src/assets/scss/argon-design-system-react.scss src/assets/css/argon-design-system-react.css --source-map true",
        "server:build": "yarn install && CI='' npm run build",
        "server:start": "pm2 deploy ecosystem.config.js production deploy",
        "docker:build": "docker build -t gettestedcovid19/webserver:latest -f docker/Dockerfile .",
        "docker:run": "docker run -p 3000:3000 gettestedcovid19/webserver:latest",
        "docker:push": "docker push gettestedcovid19/webserver:latest",
        "api:start:dev": "nodemon ./src/server/server.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "body-parser": "^1.19.0",
        "bootstrap": "4.3.1",
        "bufferutil": "^4.0.1",
        "classnames": "^2.2.6",
        "compression": "^1.7.4",
        "concurrently": "^5.1.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "fibers": "^4.0.3",
        "geo-distance-js": "~1.0.0",
        "haversine": "~1.1.1",
        "headroom.js": "0.11.0",
        "helmet": "^3.22.0",
        "mkdirp": "^1.0.4",
        "moment": "2.24.0",
        "morgan": "^1.10.0",
        "node-sass": "4.13.1",
        "nouislider": "14.1.1",
        "path": "~0.12.7",
        "pm2": "^4.2.3",
        "prerender-node": "~3.2.5",
        "query-string": "~6.11.1",
        "react": "16.13.1",
        "react-datetime": "2.16.3",
        "react-document-meta": "~3.0.0-beta.2",
        "react-dom": "16.13.1",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-scripts": "3.4.1",
        "reactstrap": "8.4.1",
        "sass": "^1.26.3",
        "socket.io": "^2.3.0",
        "socket.io-mock": "^1.2.4",
        "utf-8-validate": "^5.0.2"
    },
    "devDependencies": {
        "@types/googlemaps": "3.39.2",
        "@types/markerclustererplus": "2.1.33",
        "@types/react": "16.9.19",
        "eslint-plugin-flowtype": "3.13.0",
        "gulp": "4.0.2",
        "gulp-append-prepend": "1.0.8",
        "husky": "^4.2.5",
        "jquery": "3.4.1",
        "supertest": "^4.0.2",
        "typescript": "3.7.5"
    }
}
