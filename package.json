{
    "name": "get-tested-covid19",
    "version": "1.1.0",
    "description": "Website for the public to find COVID19 testing centers.",
    "main": "index.js",
    "repository": {
        "type": "git"
    },
    "keywords": [
        "react",
        "reactjs"
    ],
    "author": "Zach Boldyga",
    "license": "MIT",
    "homepage": "https://get-tested-covid19.org",
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "start": "env-cmd -f .env.development react-scripts start",
        "build": "env-cmd -f .env.production react-scripts build",
        "build:staging": "env-cmd -f .env.staging react-scripts build",
        "test": "react-scripts test",
        "test:dev": "react-scripts test --watchAll",
        "test:coverage": "jest --coverage",
        "docker:build": "docker build -t gettestedcovid19/webserver:latest -f docker/Dockerfile .",
        "docker:run": "docker run -p 3000:3000 gettestedcovid19/webserver:latest",
        "docker:push": "docker push gettestedcovid19/webserver:latest",
        "api:docker:build": "docker build -t gettestedcovid19/api:latest -f docker/APIDockerfile .",
        "api:docker:run": "docker run -p 5000:5000 -e NODE_ENV=production gettestedcovid19/api:latest",
        "api:docker:push": "docker push gettestedcovid19/api:latest",
        "api:start:dev": "export NODE_ENV=development&& nodemon ./src/server/server.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "dependencies": {
        "aws-sdk": "^2.753.0",
        "axios": "^0.19.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "bootstrap": "4.5.2",
        "bufferutil": "^4.0.1",
        "classnames": "^2.2.6",
        "compression": "^1.7.4",
        "concurrently": "^5.2.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "env-cmd": "^10.1.0",
        "express": "^4.17.1",
        "express-validator": "^6.5.0",
        "fibers": "^4.0.3",
        "geo-distance-js": "~1.0.0",
        "google-maps-react": "~2.0.6",
        "haversine": "~1.1.1",
        "headroom.js": "0.11.0",
        "helmet": "^3.23.1",
        "jsonwebtoken": "^8.5.1",
        "lodash.debounce": "^4.0.8",
        "mapbox-gl": "^1.10.1",
        "mkdirp": "^1.0.4",
        "moment": "2.27.0",
        "morgan": "^1.10.0",
        "node-sass": "^4.14.1",
        "normalize.css": "^8.0.1",
        "nouislider": "14.6.1",
        "passport-jwt": "^4.0.0",
        "path": "~0.12.7",
        "pg": "^8.3.3",
        "pg-hstore": "^2.3.3",
        "pm2": "^4.2.3",
        "polished": "^3.6.4",
        "prerender-node": "~3.2.5",
        "prettier": "^2.0.5",
        "query-string": "~6.13.1",
        "react": "16.13.1",
        "react-datetime": "2.16.3",
        "react-document-meta": "~3.0.0-beta.2",
        "react-dom": "16.13.1",
        "react-mapbox-gl": "^4.8.6",
        "react-router": "5.1.2",
        "react-router-dom": "5.2.0",
        "react-scripts": "3.4.1",
        "react-truncate": "^2.4.0",
        "reactstrap": "8.6.0",
        "sass": "^1.26.7",
        "sequelize": "^5.21.11",
        "sequelize-cli": "^5.5.1",
        "socket.io": "^2.3.0",
        "socket.io-mock": "^1.2.4",
        "styled-components": "^5.2.0",
        "utf-8-validate": "^5.0.2"
    },
    "devDependencies": {
        "@types/googlemaps": "3.39.2",
        "@types/markerclustererplus": "2.1.33",
        "@types/react": "16.9.19",
        "eslint-plugin-flowtype": "3.13.0",
        "gulp": "4.0.2",
        "gulp-append-prepend": "1.0.8",
        "husky": "^4.2.5",
        "jquery": "3.5.0",
        "nodemon": "^2.0.3",
        "supertest": "^4.0.2",
        "typescript": "3.7.5"
    }
}
